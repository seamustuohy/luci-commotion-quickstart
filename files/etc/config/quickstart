#----------------CHECK AND CLEAN UP---------------------
#This section sets a network and shows the user the result

config page 'configReqs'
	   option buttonText 'PINEAPPLE!'
	   option pageTitle 'REQUIRE ALL THE THINGS!' 
	   option topheader 'This network requires some things'
   	   option subheading 'THEN DO ALL THE THINGS!'
	   option nxtPg 'setConfig'
	   option fileType 'key'		
	   list modules 'configReqs'
	   list modules 'upload'

config page 'setConfig'
	   option pageTitle "Please Wait"
	   option buttonText "Accept and Set Config"
	   option topheader "Please Wait"
	   option subheading "Please reconnect to this "
	   list modules 'settingPrefs'
	   list buttons 'noNext'
	   option nxtPg 'none'
	   list buttons 'noBack'

config page 'seeNetwork'
	   option pageTitle "Connected Nodes"
	   option buttonText ""
	   option subheading "You are connected to a network that is connected to ..."
	   option nxtPg 'finish'
	   option topheader "Congratulations!"
	   list modules "connectedNodes"

config page 'finish'
	   option buttonText 'Click here to go to the admin interface'
	   list modules "complete"
	   list buttons 'noNext'
	   list buttons 'noBack'
	   option nxtPg 'none'	



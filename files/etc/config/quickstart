config quickstart options
	   option pageNo 'welcome'
	   option lastPg 'welcome'
	   option complete 'false'
	   
config page 'welcome'
	   option pageTitle 'Welcome'
	   option buttonText 'Start Over'
	   option topheader 'Welcome'
   	   option subheading 'You are only ten minutes away from setting up your node'
	   option nxtPg 'none'
	   list modules 'logo'
	   list modules 'welcome'
	   list buttons 'noBack'
	   list buttons 'noNext'


#-------NEW NETWORK PATH---------------

config page 'newNetSettings'
   option topheader 'you clicked on a network'
   option subheading 'This page also has a subheading'
   option PageTitle 'PAGE TITLES ARE IMPORTANT'
   option buttonText 'Find Nearby Networks'
   option nxtPg 'configReqs'
   list buttons 'preBuilt'
   list buttons 'noNext'
   list buttons 'haveSettingsNew'

config page 'preBuilt'
	   option buttonText 'Use a Pre-Built Config'
	   option pageTitle 'Choose a Config'
	   option topheader 'Here are some of our pre-built configurations'
   	   option subheading 'Choose one and we will continue from here'
	   option nxtPg 'newNetwork'
	   list modules 'configs'
   
config page 'haveSettingsNew'
	   option buttonText 'I have a settings file'
	   option pageTitle 'Upload A Settings File'
	   option topheader 'Upload Your Settings File Here' 
   	   option subheading 'Choose a file from your device to upload to the node.'
	   option fileType 'config'
	   option nxtPg 'newNetwork'
	   list modules 'upload'

config page 'newNetwork'
	option buttonText 'Use the one click setup'
    option pageTitle 'Quick Start'
	option topheader 'Please fill out the information below'
   	option subheading 'This will allow us to configure your node'
	option nxtPg 'configReqs'
	list modules 'basicInfo'
	list modules 'oneClick'
	list modules 'networkRandomizer'

#----------Join Network Path
config page 'joinMesh'
       option topheader 'Here are the nearby networks.'
       option subheading 'Click on a network to make a compatible node.'
       option PageTitle 'Nearby Networks'
       option buttonText 'Find Nearby Networks'
	   option nxtPg 'joinSetup'
       list modules 'nearbyMesh'

config page 'joinSetup'
	option buttonText 'This Button Leads to nameing and password stuff'
    option pageTitle 'I AM TITLED THINGS'
	option topheader 'Please fill out the information below'
   	option subheading 'This will allow us to configure your node'
	option nxtPg 'settings'
	list modules 'basicInfo'
	   
